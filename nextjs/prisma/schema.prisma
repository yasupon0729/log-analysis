generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model JudgeImage {
  id               String   @id @default(uuid())
  userId           String //ユーザーID
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
  analysisId       String //解析ID
  originalImageUrl String //元画像URL
  maskImageUrl     String //マスク画像URL
  isExcel          Boolean  @default(false)
  point            Int? //点数(0~100)
  note             String? //備考

  @@index([userId, analysisId])
  @@unique([userId, analysisId, originalImageUrl])
}
